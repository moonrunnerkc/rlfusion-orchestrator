{
  "__meta": {
    "author": "Bradley R. Kinnard",
    "description": "Grafana dashboard template for RLFusion Orchestrator monitoring"
  },
  "annotations": {
    "list": []
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "title": "Query Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(rlfusion_query_latency_seconds_bucket[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(rlfusion_query_latency_seconds_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(rlfusion_query_latency_seconds_bucket[5m]))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 15 }
            ]
          }
        }
      }
    },
    {
      "title": "Retrieval Path Usage",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(rlfusion_retrieval_path_total{path=\"rag\"}[5m])",
          "legendFormat": "RAG"
        },
        {
          "expr": "rate(rlfusion_retrieval_path_total{path=\"cag\"}[5m])",
          "legendFormat": "CAG"
        },
        {
          "expr": "rate(rlfusion_retrieval_path_total{path=\"graph\"}[5m])",
          "legendFormat": "Graph"
        },
        {
          "expr": "rate(rlfusion_retrieval_path_total{path=\"web\"}[5m])",
          "legendFormat": "Web"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        }
      }
    },
    {
      "title": "Fusion Weight Distribution",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(rlfusion_fusion_weight_bucket{path=\"rag\"}[5m]))",
          "legendFormat": "RAG median"
        },
        {
          "expr": "histogram_quantile(0.50, rate(rlfusion_fusion_weight_bucket{path=\"cag\"}[5m]))",
          "legendFormat": "CAG median"
        },
        {
          "expr": "histogram_quantile(0.50, rate(rlfusion_fusion_weight_bucket{path=\"graph\"}[5m]))",
          "legendFormat": "Graph median"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        }
      }
    },
    {
      "title": "Critique Reward Distribution",
      "type": "histogram",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rate(rlfusion_critique_reward_bucket[5m])",
          "legendFormat": "{{le}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      }
    },
    {
      "title": "Safety Gate Triggers",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "increase(rlfusion_safety_gate_triggers_total[1h])",
          "legendFormat": "Blocked (1h)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 5 },
              { "color": "red", "value": 20 }
            ]
          }
        }
      }
    },
    {
      "title": "Replay Buffer Size",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rlfusion_replay_buffer_size",
          "legendFormat": "Episodes"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 100 },
              { "color": "yellow", "value": 5000 }
            ]
          }
        }
      }
    },
    {
      "title": "Active WebSocket Connections",
      "type": "stat",
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "rlfusion_ws_connections_active",
          "legendFormat": "Active WS"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 10 },
              { "color": "red", "value": 50 }
            ]
          }
        }
      }
    },
    {
      "title": "HTTP Request Rate",
      "type": "timeseries",
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "targets": [
        {
          "expr": "sum(rate(rlfusion_http_requests_total[5m])) by (endpoint)",
          "legendFormat": "{{endpoint}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      }
    }
  ],
  "refresh": "10s",
  "schemaVersion": 39,
  "tags": ["rlfusion", "monitoring", "phase9"],
  "templating": {
    "list": [
      {
        "name": "DS_PROMETHEUS",
        "type": "datasource",
        "query": "prometheus",
        "current": { "text": "Prometheus", "value": "prometheus" }
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "title": "RLFusion Orchestrator",
  "uid": "rlfusion-orchestrator-v1",
  "version": 1
}
